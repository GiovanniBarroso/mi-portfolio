{
  "name": "mi-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,vue,css,md}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tailwind:init": "tailwindcss init -p",
    "lint": "eslint . --ext .ts,.vue --max-warnings 0",
    "format": "prettier -w .",
    "typecheck": "vue-tsc -b",
    "prepare": "husky",
    "ci:check": "pnpm lint && pnpm typecheck && pnpm build"
  },
  "dependencies": {
    "@unhead/vue": "^1.11.20",
    "@vueuse/core": "^14.0.0",
    "@vueuse/motion": "^3.0.3",
    "oh-vue-icons": "1.0.0-rc3",
    "unhead": "^1.11.20",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.3",
    "vite": "^7.2.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.4"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
